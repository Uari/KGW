<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<mapper namespace="com.mybatis.mapper.KiwoomNoticeMapper">

<select id="kiwoomNoticeList" parameterType="com.vo.KiwoomNoticeVO" resultType="com.vo.KiwoomNoticeVO">
SELECT board_no,
       emp_no,
       board_title,
       board_content,
       reg_date,
       board_hit,
       mod_date
    FROM media_board
</select>
<delete id="kiwoomNoticeDelete" parameterType="int">
DELETE FROM media_board WHERE board_no= #{board_no}

</delete>
</mapper>